    "\n",
    "def prime_factors(n):\n",
    "    factors = []\n",
    "    # Remove 2s first (even numbers)\n",
    "    while n % 2 == 0:\n",
    "        factors.append(2)\n",
    "        n //= 2\n",
    "\n",
    "    # Check odd numbers up to sqrt(n)\n",
    "    i = 3\n",
    "    while i * i <= n:\n",
    "        while n % i == 0:\n",
    "            factors.append(i)\n",
    "            n //= i\n",
    "        i += 2\n",
    "\n",
    "    # If anything remains, it’s a prime\n",
    "    if n > 2:\n",
    "        factors.append(n)\n",
    "\n",
    "    return factors"
